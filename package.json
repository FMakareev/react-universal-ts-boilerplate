    "start:ssr:server": "babel-node .scripts/ssr/start.ssr.server.js",
    "test": "jest --config=jest.config.js",
    "test:snapshot:update": "jest --config=jest.config.js --updateSnapshot",
    "test:sb:snapshot": "jest --config ./jest.sb.config.js",
    "test:sb:snapshot:update": "jest --config=jest.sb.config.js --updateSnapshot",
    "check-merge-conflict": "babel-node ./tools/checkMergeConflict.js",
    "pre-commit": "lint-staged",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "format": "prettier --config ./.prettierrc.js --write --list-different \"src/**/*.{ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "run-jscpd": "npx jscpd ./ --ignore \"**/node_modules/**,**/build/**,**/storybook-static/**,**/dist/**\" -r html,console -o \"./jscpd-report/\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run format",
      "git add"
    ]
