    "build:csr:prod": "babel-node .scripts/csr/build.csr.js --mode=production --watch=false",
    "build:csr:dev": "babel-node .scripts/csr/build.csr.js --mode=development --watch=false",
    "start:csr:server": "babel-node .scripts/csr/start.csr.server.js",
    "build:ssr:prod": "babel-node .scripts/ssr/build.ssr.js --mode=production  --watch=false",
    "build:ssr:dev": "babel-node .scripts/ssr/build.ssr.js --mode=development  --watch=false",
    "start:ssr:prod": "babel-node .scripts/ssr/start.ssr.js --mode=development --watch=true",
